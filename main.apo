enum Result {
	Ok(i4)
	Err(string)
}

fn print(self: Result) {
	if self.Ok(result) {
		printf("Ok %d\n", result)
	} else if self.Err(error) {
		printf("Err %s\n", error)
	}
	return
}

fn div(one: i4, two: i4) -> Result {
	if two == 0 {
		return Result.Err("Cannot divide by zero")
	}
	return Result.Ok(one / two)
}

let result = div(4, 2)
result.print()
