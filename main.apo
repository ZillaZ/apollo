use static std::threads::threads

fn hello(name: Any) {
  printf("Hello, %s!\n", name as string)
  threads::thread_exit(name)
  return
}

let id : u8 = 2469
let name = "Lucas"
let result : string

threads::thread_spawn(&id, hello, name)
threads::thread_join(id, &result)
printf("Result is %s\n", result)
