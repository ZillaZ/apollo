use static std::threads::threads
use static std::sockets::sockets
use static std::strings::strings
use static std::http::parser

fn read(stream: Any) {
	let stream = stream as &TcpStream
	let data = read_tcp(stream)
	parse_request(data)
	thread_exit(stream)
	return
}

let address = "127.0.0.1:8080"
let listener = new_tcp_listener(address)
let id : u8 = 0
while (true) {
	let stream = receive_incoming(listener)
	printf("received connection\n")
	id += 1
	thread_spawn(&id, &read, stream)
}
